{
	"name": "stallion",
	"version": "1.0.0",
	"author": "Steadfast Design Firm",
	"license": "UNLICENSED",
	"private": true,
	"scripts": {
		"dev": "nodemon",
		"prod": "webpack --config config/build/webpack.prod.js --progress",
		"icons": "./node_modules/svgo/bin/svgo --recursive --multipass --folder=web/assets/icons --config=./config/build/svgo.yml"
	},
	"nodemonConfig": {
		"watch": [
			"config/build/webpack.common.js",
			"config/build/webpack.dev.js"
		],
		"exec": "webpack serve --config config/build/webpack.dev.js"
	},
	"dependencies": {
		"tailwindcss": "^2.0.2",
		"vue": "^2.6.14"
	},
	"devDependencies": {
		"@babel/core": "^7.12.9",
		"@babel/plugin-transform-runtime": "^7.12.1",
		"@babel/preset-env": "^7.12.7",
		"autoprefixer": "^10.0.4",
		"babel-loader": "^8.2.2",
		"clean-webpack-plugin": "^3.0.0",
		"css-loader": "^5.0.1",
		"cssnano": "^4.1.10",
		"html-loader": "^1.3.2",
		"less": "^3.12.2",
		"less-loader": "^7.1.0",
		"mini-css-extract-plugin": "^1.3.2",
		"nodemon": "^2.0.6",
		"postcss": "^8.2.1",
		"postcss-import": "^13.0.0",
		"postcss-loader": "^4.1.0",
		"postcss-nested": "^5.0.1",
		"style-loader": "^2.0.0",
		"svgo": "^1.3.0",
		"vue-loader": "^15.9.5",
		"vue-template-compiler": "^2.6.12",
		"webpack": "^5.9.0",
		"webpack-cli": "^4.2.0",
		"webpack-dev-server": "^4.0.0-beta.0",
		"webpack-manifest-plugin": "^3.0.0",
		"webpack-merge": "^5.4.0"
	},
	"browserslist": {
		"legacyBrowsers": [
			"> 1%",
			"last 2 versions",
			"Firefox ESR"
		],
		"modernBrowsers": [
			"last 2 Chrome versions",
			"not Chrome < 60",
			"last 2 Safari versions",
			"not Safari < 10.1",
			"last 2 iOS versions",
			"not iOS < 10.3",
			"last 2 Firefox versions",
			"not Firefox < 54",
			"last 2 Edge versions",
			"not Edge < 15"
		],
		"production": [
			"> 1%",
			"last 2 versions",
			"Firefox ESR"
		]
	}
